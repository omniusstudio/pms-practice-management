{
  "dashboard": {
    "id": null,
    "title": "Mental Health PMS - Frontend Metrics Dashboard",
    "tags": ["pms", "frontend", "performance", "hipaa"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Page Load Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(pms_frontend_page_load_time_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pms_frontend_page_load_time_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pms_frontend_page_load_time_bucket[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Frontend Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_frontend_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "API Call Performance (Frontend)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(pms_frontend_api_call_duration_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pms_frontend_api_call_duration_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "User Actions per Minute",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pms_frontend_user_actions_total[1m]) * 60",
            "legendFormat": "Actions/min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Active Users (Estimated)",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (correlation_id) (rate(pms_frontend_user_actions_total[5m]) > 0))",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Component Errors (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(pms_frontend_component_errors_total[24h])",
            "legendFormat": "Component Errors",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 20
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Frontend SLO (Page Load < 2s)",
        "type": "stat",
        "targets": [
          {
            "expr": "(rate(pms_frontend_page_load_time_bucket{le=\"2000\"}[24h]) / rate(pms_frontend_page_load_time_count[24h])) * 100",
            "legendFormat": "SLO Compliance %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent",
            "max": 100,
            "min": 80
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Most Active Components",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (component) (rate(pms_frontend_user_actions_total[1h])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "component": "Component",
                "Value": "Actions/sec"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 9,
        "title": "Slowest Pages (95th percentile)",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum by (page) (rate(pms_frontend_page_load_time_bucket[1h]))))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "page": "Page",
                "Value": "Load Time (ms)"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "Long Tasks (Performance Issues)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_frontend_long_task_total[5m])",
            "legendFormat": "Long Tasks/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 0.5
                }
              ]
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 11,
        "title": "Layout Shifts (CLS)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_frontend_layout_shift_total[5m])",
            "legendFormat": "Layout Shifts/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.05
                },
                {
                  "color": "red",
                  "value": 0.1
                }
              ]
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": 12,
        "title": "Form Interactions",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_frontend_form_interactions_total[5m])",
            "legendFormat": "{{interaction_type}} - {{form}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 34
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "definition": "label_values(pms_frontend_page_load_time_count, environment)",
          "hide": 0,
          "includeAll": true,
          "label": "Environment",
          "multi": true,
          "name": "environment",
          "options": [],
          "query": {
            "query": "label_values(pms_frontend_page_load_time_count, environment)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "definition": "label_values(pms_frontend_page_load_time_count{environment=~\"$environment\"}, page)",
          "hide": 0,
          "includeAll": true,
          "label": "Page",
          "multi": true,
          "name": "page",
          "options": [],
          "query": {
            "query": "label_values(pms_frontend_page_load_time_count{environment=~\"$environment\"}, page)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "enable": true,
          "expr": "rate(pms_frontend_errors_total[5m]) > 0.1",
          "iconColor": "red",
          "name": "High Frontend Error Rate",
          "titleFormat": "Frontend Error Spike",
          "textFormat": "High frontend error rate detected: {{value}} errors/sec"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "enable": true,
          "expr": "histogram_quantile(0.95, rate(pms_frontend_page_load_time_bucket[5m])) > 3000",
          "iconColor": "orange",
          "name": "Slow Page Loads",
          "titleFormat": "Performance Degradation",
          "textFormat": "95th percentile page load time: {{value}}ms"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "weekStart": ""
  }
}
