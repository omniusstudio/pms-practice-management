[flake8]
max-line-length = 88
max-complexity = 10
select = E,W,F
ignore = E203,E501,W503
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    .mypy_cache,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .coveragerc,
    pytest.ini,
    setup.py,
    conftest.py