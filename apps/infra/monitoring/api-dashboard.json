{
  "dashboard": {
    "id": null,
    "title": "Mental Health PMS - API Metrics Dashboard",
    "tags": ["pms", "api", "red-metrics", "hipaa"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (Requests/sec)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Error Rate (%)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_http_requests_total{status_code=~\"4..|5..\"}[5m]) / rate(pms_http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Response Time (Duration)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(pms_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pms_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pms_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "pms_http_requests_active",
            "legendFormat": "{{environment}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Total Requests (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(pms_http_requests_total[24h])",
            "legendFormat": "Total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Error Count (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(pms_http_errors_total[24h])",
            "legendFormat": "Errors",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "SLO Compliance (99.9% Uptime)",
        "type": "stat",
        "targets": [
          {
            "expr": "(1 - (rate(pms_http_requests_total{status_code=~\"5..\"}[24h]) / rate(pms_http_requests_total[24h]))) * 100",
            "legendFormat": "Uptime %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 99.5
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent",
            "max": 100,
            "min": 99
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Top Endpoints by Request Count",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint, method) (rate(pms_http_requests_total[1h])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "endpoint": "Endpoint",
                "method": "Method",
                "Value": "Requests/sec"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 9,
        "title": "Slowest Endpoints (95th percentile)",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum by (endpoint, method) (rate(pms_http_request_duration_seconds_bucket[1h]))))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "endpoint": "Endpoint",
                "method": "Method",
                "Value": "Duration (s)"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "Audit Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_audit_events_total[5m])",
            "legendFormat": "{{event_type}} - {{resource_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 11,
        "title": "Authentication Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pms_auth_events_total[5m])",
            "legendFormat": "{{event_type}} - Success: {{success}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": 12,
        "title": "PHI Scrubbing Operations",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(pms_phi_scrub_total[24h])",
            "legendFormat": "{{scrub_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 34
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "definition": "label_values(pms_http_requests_total, environment)",
          "hide": 0,
          "includeAll": true,
          "label": "Environment",
          "multi": true,
          "name": "environment",
          "options": [],
          "query": {
            "query": "label_values(pms_http_requests_total, environment)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "definition": "label_values(pms_http_requests_total{environment=~\"$environment\"}, endpoint)",
          "hide": 0,
          "includeAll": true,
          "label": "Endpoint",
          "multi": true,
          "name": "endpoint",
          "options": [],
          "query": {
            "query": "label_values(pms_http_requests_total{environment=~\"$environment\"}, endpoint)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "enable": true,
          "expr": "changes(pms_version_info[1m]) > 0",
          "iconColor": "green",
          "name": "Deployments",
          "titleFormat": "Deployment: {{version}}",
          "textFormat": "Service: {{service}}, Environment: {{environment}}"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "enable": true,
          "expr": "rate(pms_http_requests_total{status_code=~\"5..\"}[5m]) > 0.1",
          "iconColor": "red",
          "name": "High Error Rate",
          "titleFormat": "Error Spike",
          "textFormat": "High error rate detected: {{value}} errors/sec"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 37,
    "version": 1,
    "weekStart": ""
  }
}
